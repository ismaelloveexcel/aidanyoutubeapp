{
  "name": "tubestar-creator-studio",
  "version": "1.0.0",
  "description": "A YouTube video creation helper app for 8-12 year old kids",
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "prebuild": "mkdir -p dist",
    "build": "npm run prebuild && npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "npx esbuild server/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.cjs --packages=external",
    "postbuild": "node -e \"const fs = require('fs'); if (!fs.existsSync('dist/index.cjs')) { console.error('Build failed: dist/index.cjs not found'); process.exit(1); } console.log('Build verified: dist/index.cjs exists (' + Math.round(fs.statSync('dist/index.cjs').size / 1024) + ' KB)');\"",
    "start": "NODE_ENV=production node dist/index.cjs",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.62.11",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.1",
    "@uppy/aws-s3": "^5.1.0",
    "@uppy/core": "^5.2.0",
    "@uppy/dashboard": "^5.1.0",
    "@uppy/react": "^5.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.38.0",
    "drizzle-zod": "^0.6.1",
    "esbuild": "^0.20.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^11.15.0",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.562.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.21",
    "vite-tsconfig-paths": "^4.3.1",
    "wouter": "^3.3.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "drizzle-kit": "^0.29.1"
  }
}
